<div>
  <h1>{{title}}</h1>
    <form>
      <div class="container">
        <img src = "resources/Minimal-Magnifying-Glass.png" alt = "magnifying glass image">
      </div>
      <div class ="container">
        <p class="search-text">Search for a repository.</p>
      </div>
      <div class="container">
            <input type="text" name="search" class ="searchbar" placeholder="Search..">
      </div>
      <div class="container">
        <button type="submit" class="btn btn-primary">Submit</button>
      </div>
      <div class="container">
        <p>language:</p>
        <select>
          <option>Java</option>
          <option>Javascript</option>
          <option>C++</option>
          <option>C#</option>
          <option>Ruby</option>
          <option>C</option>
          <option>Python</option>
          <option>Swift</option>
          <option>R</option>
          <option>HTML</option>
          <option>CSS</option>
        </select>
      </div>
      <div class="container">
        <p>license:</p>
        <select>
          <option>MIT</option>
          <option>GNU</option>
        </select>
      </div>
    </form>
</div>
